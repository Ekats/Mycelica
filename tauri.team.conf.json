{
  "productName": "Mycelica Team",
  "mainBinaryName": "mycelica-team",
  "identifier": "com.mycelica.team",
  "build": {
    "beforeDevCommand": "cd src-team && npm run dev",
    "devUrl": "http://localhost:1421",
    "beforeBuildCommand": "cd src-team && npm run build",
    "beforeBundleCommand": "rm -f src-tauri/target/release/batch_import src-tauri/target/release/mycelica src-tauri/target/release/mycelica-server src-tauri/target/release/mycelica-cli && ln -sf mycelica-cli src-tauri/target/release/cli",
    "frontendDist": "../src-team/dist"
  },
  "app": {
    "windows": [
      {
        "title": "Mycelica Team",
        "width": 1400,
        "height": 900,
        "minWidth": 900,
        "minHeight": 600
      }
    ]
  },
  "bundle": {
    "targets": ["deb"],
    "externalBin": [],
    "resources": []
  }
}
